{% for emp in query %}
<div class="col-xl-4 col-lg-4 col-md-6">
    <div class="team-box text-center mb-80">
        <div class="team-thumb mb-45 pos-rel">
            <img src="{{emp.cover}}" alt="" id="team-image" />
            <a class="team-link" href="/doctor/{{emp.slug}}">+</a>
        </div>
        <div class="team-content">
            <h3><a href="/doctor/{{emp.slug}}">{{emp.name}}</a></h3>
            <h6>{{emp.department_name}}</h6>
        </div>
    </div>
</div>
{% endfor %}

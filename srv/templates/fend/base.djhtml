    <script>
      function doad(id, api, body) {
              fetch(api, {
                  method  : 'POST',
                  body    : body,
              }).then(function(response) {
                  if (!response.ok) return
                  response.text().then(function(responseText) {
                      document.getElementById(id).innerHTML = responseText
                  })
              })
      }
  </script>
